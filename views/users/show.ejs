
<h1>User Data for <%= user.fullName %></h1>
<table>
  <tr>
    <th>Name</th>
    <td><%= user.name %></td>
  </tr>
  <tr>
    <th>Email</th>
    <td><%= user.email %></td>
  </tr>
  <tr>
    <th>Zip Code</th>
    <td><%= user.zipCode %></td>
  </tr>
  <tr>
    <th>Password</th>
    <td><%= user.password %></td>
  </tr>
  <tr>
    <th>Token</th>
    <td><%= user.apiToken %></td>
  </tr>
</table>
<% if (user.subscribedAccount) { %>
<h4 class="center">
  This user has a subscribed account</a
  >.
</h4>
<% } %>


<div>
  <a href="/users">View all users</a>
</div>
<div>
  <a href="<%=`/users/${user._id}/edit`%>">Edit User Details</a>
</div>
<div>
  <a href="<%= `/users/${user._id}/delete?_method=DELETE` %>" onclick="return confirm('Are you sure you want to delete this record?')">Delete</a>
</div>